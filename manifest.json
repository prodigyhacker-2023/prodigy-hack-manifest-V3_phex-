{
  "manifest_version": 3,
  "name": "prodigy_hacker",
  "version": "2.1.9",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["disableIntegrity.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "deafult_title": "hackr"
  },
  "permissions": ["https://sso.prodigygame.com"]
}
